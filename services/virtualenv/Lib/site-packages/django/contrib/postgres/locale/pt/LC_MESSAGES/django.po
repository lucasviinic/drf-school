#: .\virtualenv\Lib\site-packages\django\contrib\postgres\apps.py:42
msgid "PostgreSQL extensions"
msgstr "Extensões de PostgresSQL"

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\fields\array.py:21
#: .\virtualenv\Lib\site-packages\django\contrib\postgres\forms\array.py:16
#: .\virtualenv\Lib\site-packages\django\contrib\postgres\forms\array.py:170
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Item %(nth)s na lista não validou:"

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\fields\array.py:22
msgid "Nested arrays must have the same length."
msgstr "As sub-listas têm de ter o mesmo tamanho."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\fields\hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Mapeamento de strings para strings/nulos"

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\fields\hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr ""

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\forms\hstore.py:16
msgid "Could not load JSON data."
msgstr "Não foi possível carregar os dados JSON."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\forms\hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "A entrada deve ser um dicionário JSON."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:33
msgid "Enter two valid values."
msgstr "Introduza dois valores válidos."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:34
msgid "The start of the range must not exceed the end of the range."
msgstr "O início da gama não pode ser maior que o seu fim."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:82
msgid "Enter two whole numbers."
msgstr "Introduza dois números inteiros."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:88
msgid "Enter two numbers."
msgstr "Introduza dois números."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:94
msgid "Enter two valid date/times."
msgstr "Introduza duas datas/horas válidas."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:100
msgid "Enter two valid dates."
msgstr "Introduza duas datas válidas."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\validators.py:12
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"A lista contém %(show_value)d item, não pode conter mais do que "
"%(limit_value)d."
msgstr[1] ""
"A lista contém %(show_value)d itens, não pode conter mais do que "
"%(limit_value)d."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\validators.py:19
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"A lista contém %(show_value)d item, tem de conter pelo menos %(limit_value)d."
msgstr[1] ""
"A lista contém %(show_value)d itens, tem de conter pelo menos "
"%(limit_value)d."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\validators.py:29
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Algumas chaves estão em falta: %(keys)s"

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\validators.py:30
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Foram fornecidas algumas chaves desconhecidas: %(keys)s"

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\validators.py:70
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr "Garanta que esta gama é toda ela menor ou igual a %(limit_value)s."

#: .\virtualenv\Lib\site-packages\django\contrib\postgres\validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr "Garanta que esta gama é toda ela maior ou igual a %(limit_value)s."
