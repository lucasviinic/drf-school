#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\admin.py:12
msgid "Advanced options"
msgstr "Opções avançadas"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\apps.py:7
msgid "Flat Pages"
msgstr "Páginas Planas"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\forms.py:10
#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:9
msgid "URL"
msgstr "URL"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\forms.py:13
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Exemplo: “/about/contact/”. Assegure que se tenha barras no início e no "
"final."

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\forms.py:16
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Este valor deve conter apenas letras, números, pontos, sublinhados, traços, "
"barras ou til."

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\forms.py:30
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr "Exemplo: “/about/contact”. Assegure que se tenha uma barra no início."

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\forms.py:43
msgid "URL is missing a leading slash."
msgstr "Está faltando uma barra no início da URL."

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\forms.py:48
msgid "URL is missing a trailing slash."
msgstr "Está faltando uma barra no final da URL."

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\forms.py:65
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Uma flatpage com a URL %(url)s já existe para o site %(site)s"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:10
msgid "title"
msgstr "título"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:11
msgid "content"
msgstr "conteúdo"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:12
msgid "enable comments"
msgstr "habilitar comentários"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:14
msgid "template name"
msgstr "nome do template"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Exemplo: “flatpages/contact_page.html”. Se isso não for fornecido, o sistema "
"utilizará “flatpages/default.html”."

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:23
msgid "registration required"
msgstr "registro obrigatório"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:24
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Se estiver assinalado, apenas usuários autenticados poderão ver a página."

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:27
msgid "sites"
msgstr "sites"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:31
msgid "flat page"
msgstr "página plana"

#: .\virtualenv\Lib\site-packages\django\contrib\flatpages\models.py:32
msgid "flat pages"
msgstr "páginas planas"
